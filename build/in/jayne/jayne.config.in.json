{
  "CorsConfig": {
    "AllowedOrigins": [
      "{{ESTATE_DOCSITE_URL}}"
    ]
  },
  "LoggingConfig": {
    "MinLevel": "{{ESTATE_JAYNE_LOG_MIN_LEVEL}}",
    "SystemLevel": "{{ESTATE_JAYNE_LOG_SYSTEM_LEVEL}}",
    "MicrosoftHostingLifetimeLevel": "{{ESTATE_JAYNE_LOG_MICROSOFTHOSTINGLIFETIME_LEVEL}}",
    "MicrosoftAspNetCoreLevel": "{{ESTATE_JAYNE_LOG_MICROSOFTASPNETCORE_LEVEL}}",
    "MicrosoftEntityFrameworkCoreDatabaseCommandLevel": "{{ESTATE_JAYNE_LOG_MICROSOFTENTITYFRAMEWORKCOREDATABASECOMMAND_LEVEL}}"
  },
  "FirebaseConfig": {
    "FirebaseProject": "{{ESTATE_FIREBASE_PROJECT_ID}}"
